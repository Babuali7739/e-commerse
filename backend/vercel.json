{
    "version": 2,
    "builds": [
      {
        "src": "/index.js", 
        "use": "@vercel/node"  
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js" 
      }
    ],
    "env": {
      "NODE_ENV": "production" 
    },
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs16.x"
      }
    },
    "buildCommand": "npm run build", 
    "outputDirectory": "dist"
  
}  